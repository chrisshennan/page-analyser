{
    "name": "chrisshennan/page-analyser",
    "type": "library",
    "description": "A simple, extendable page analyser written in PHP",
    "keywords": [
        "canonical",
        "analyser",
        "meta tags",
        "json-ld",
        "schema.org"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Shennan",
            "email": "chris@chrisshennan.com",
            "homepage": "https://www.chrisshennan.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Cas\\PageAnalyser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
